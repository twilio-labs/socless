{
  "name": "socless",
  "version": "1.0.0",
  "description": "Socless' core infrastructure and functionality",
  "main": "index.js",
  "config": {
    "aws_profile": "--aws-profile default",
    "dev": "--stage dev --region us-west-2",
    "stage": "--stage stage --region us-east-2",
    "prod": "--stage prod --region us-east-1",
    "sandbox": "--stage sandbox --region us-west-1"
  },
  "scripts": {
    "test": ". venv/bin/activate && tox",
    "build-funcs": "node node_modules/lambda-packager/build/src/main.js functions -c common -o .archives/functions -u 'no-linux'",
    "build-layers": "node node_modules/lambda-packager/build/src/main.js layers -o .archives/layers -u 'no-linux' || true",
    "build-all": "$npm_execpath run build-funcs && $npm_execpath run build-layers",
    "pretest": "python3 -m venv venv && . venv/bin/activate && pip3 install tox -qq",
    "posttest": "find . | grep -E '(__pycache__|.pytest_cache|.egg-info)' | xargs rm -rf",
    "dev": "$npm_execpath run build-all && serverless deploy $npm_package_config_dev $npm_package_config_aws_profile --verbose ",
    "stage": "$npm_execpath run build-all && serverless deploy $npm_package_config_stage $npm_package_config_aws_profile --verbose",
    "prod": "$npm_execpath run build-all && serverless deploy $npm_package_config_prod $npm_package_config_aws_profile --verbose",
    "sandbox": "$npm_execpath run build-all && serverless deploy $npm_package_config_sandbox $npm_package_config_aws_profile --verbose"
  },
  "devDependencies": {},
  "dependencies": {
    "serverless": "2.57.0",
    "lambda-packager": "twilio-labs/socless_lambda_packager#v0.1.0",
    "sls-apb": "twilio-labs/sls-apb#1.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/twilio-labs/socless.git"
  },
  "keywords": [
    "socless",
    "security",
    "automation"
  ],
  "author": "Ubani Balogun",
  "license": "ISC"
}